<wxs module="filter" src="../../utils/getdatr.wxs">
</wxs>
<block wx:if="{{isHospital}}">
  <navigator hover-class='none' url='../hosdetail/hosdetail?id={{item.id}}' class='hospitals r1 mt30' wx:for="{{list}}" wx:key="{{item.id}}">
    <view class='col hos-head'>
      <image mode='aspectFill' class='hos-headimg' src='{{item.headimg.path}}'></image>
      <view class='hos-score'>{{item.score}}</view>
    </view>
    <view class='col hos-na'>
      <view class='hos-name f32'>{{item.name}}</view>
      <text wx:if="{{item.address.length<45}}" class='hos-address le1 f28'>{{item.address}}</text>
      <text wx:else class='hos-address le1 f28'>{{filter.Limit3(item.address,45)}}</text>
    </view>
    <image class='hosimg' src='../../images/next.png'></image>
  </navigator>
</block>
<block wx:else>
  <include src="ency.wxml" />
</block>